{
    "name": "silverstripers/custom-emails",
    "description": "Send fully configurable custom emials from Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "emails", "module"],
    "license": "BSD-3-Clause",
    "authors": [
    {
        "name": "Nivanka Fonseka",
        "email": "nivanka@silverstripers.com"
    }],
    "require": {
        "silverstripe/framework": "^4.10",
        "silverstripe/cms": "^4.10"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ],
        "installer-name": "custom-emails"
    },
    "autoload": {
        "psr-4": {
            "SilverStripers\\CustomEmails\\": "src/"
        }
    }
}
